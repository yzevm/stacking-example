{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "chai-friendly"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "rules": {
        "prettier/prettier": 1,
        "no-unused-expressions": 0,
        "chai-friendly/no-unused-expressions": 2,
        "@typescript-eslint/explicit-function-return-type": 0
    }
}